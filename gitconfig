[alias]
    box = !open -a Gitbox .
    ci = commit
    co = checkout
    d = diff
    dc = diff --cached
    graph = log --graph --oneline
    out = log origin/master..HEAD
    record = add -p
    st = status --branch --short
    unstage = reset HEAD --
    unstash = stash pop

[core]
    pager = cat

[push]
    default = tracking
